                altFormat: "d-m-Y H:i",
                dateFormat: "Y-m-d H:i",
                time_24hr:  true,
                locale: 'es',
            });

            $(".datepicker").flatpickr({
                enableTime: false,
                altFormat: "d-m-Y",
                dateFormat: "Y-m-d",
                time_24hr:  true,
                locale: 'es',
